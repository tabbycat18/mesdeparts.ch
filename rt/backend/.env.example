# --------------------------------------------------
# Backend environment variables (example)
# --------------------------------------------------

# PostgreSQL / Neon connection string
# Example: postgres://user:password@host:5432/dbname
DATABASE_URL='postgres://user:password@host:5432/dbname?sslmode=require'

# Enable GTFS Realtime processing
ENABLE_RT=1

# GTFS-RT access token (if your provider requires one)
# Example: Authorization: Bearer <token>
GTFS_RT_TOKEN=replace-with-token

# Optional: environment
NODE_ENV=development

GTFS_RT_MAX_CALLS_PER_MIN=5
GTFS_RT_CACHE_MS=12000
DEBUG_RT=0
